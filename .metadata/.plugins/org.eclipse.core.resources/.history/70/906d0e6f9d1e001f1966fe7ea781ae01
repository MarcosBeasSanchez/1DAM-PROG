package examen2Final;

import java.util.ArrayList;
import java.util.List;

enum Tipo {
    LINEA(1),
    PUNTOS(0.50),
    LINEA_PUNTEADA(0.75);

    private final double value;

    Tipo(double value) {
        this.value = value;
    }

    public double getValue() {
        return value;
    }
    
    public int cuantos(){
    	int c = 0;
    	Tipo[]tipo = Tipo.values();
    	
    	for (int i = 0; i < Tipo.values().length; i++) {
    		c++;
		}
    	
    	return c;
    	
    }
}