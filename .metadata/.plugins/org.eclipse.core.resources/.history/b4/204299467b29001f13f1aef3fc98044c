package ejercicio1ex;

public enum Tipo {
	
	COMEDIA (0), TERROR(0.5f) , AVENTURA (0.75f), NOVELA (1);
	
	private final float precio;

	Tipo(float precio) {
		this.precio = precio;
	}

	public float getPrecio() {
		return precio;
	}
	
	public comprobar(Tipo t) {
		
		for (Tipo t : Tipo.values()) {
	        if (t == tipo) {
	            tipo Valido = true;
	            break;
	        }
	}
	}
		

}
